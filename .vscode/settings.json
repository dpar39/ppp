        "coroutine": "cpp",
        "*.ipp": "cpp"
    },
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "./thirdparty/android-ndk-r15c": true,
        "./thirdparty/sdk-tools-*": true,
        "./thirdparty/gradle-*": true
    },
    "search.exclude": {
        "**/node_modules": true,
        "**/bower_components": true
    },
    "files.trimTrailingWhitespace": true,
    "editor.codeLens": false,
    "editor.autoIndent": true,
    "python.linting.pycodestyleEnabled": true,
    "python.linting.pycodestyleArgs": [
        "--ignore=E501",
        "--max-line-length=100"
    ],
    "prettier.singleQuote": true,
    "prettier.printWidth": 100,
    "cSpell.words": [
        "Rodrigues",
        "elmt",
        "emscripten",
        "exif",
        "greenyellow",
        "isdir",
        "onend",
        "onmove",
        "onstart",
        "opencv",
        "wasm",
        "webapp"
    ],
    "catch2TestExplorer.executables": [
        {
            "name": "Unit Tests Debug",
            "description": "${relDirpath}/",
            "pattern": "{build_*debug*}/libppp/*{_test}*"
        },
        {
            "name": "Unit Tests Release",
            "description": "${relDirpath}/",
            "pattern": "{build_*release*}/libppp/*{_test}*"
        }
    ],
    "catch2TestExplorer.debugConfigTemplate": {
        "type": "cppdbg",
        "MIMode": "lldb",
        "program": "${exec}",
        "args": "${args}",
        "cwd": "${cwd}",
        "env": "${envObj}",
        "externalConsole": false
    },
    "cmake.buildDirectory": "${workspaceRoot}/build_darwin_debug_x64",
    "cmake.installPrefix": "${workspaceRoot}/install_darwin_debug_x64",
    "cmake.generator": "Ninja",
    "python.formatting.provider": "autopep8",
    "python.formatting.autopep8Args": [
        "--max-line-length=110"
    ],
    "cmake.configureOnOpen": false,
    "python.pythonPath": "/usr/local/bin/python3",
    "C_Cpp.default.configurationProvider": "vector-of-bool.cmake-tools"
}